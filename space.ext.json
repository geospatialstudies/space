{
    "type": "CityJSONExtension",
    "name": "3DSpace",
    "description": "Extension for 3D modeling of interests on space exploration",
    "url": "https://raw.githubusercontent.com/geospatialstudies/space/refs/heads/main/space.ext.json",
    "version": "1.0",
    "extraAttributes": {
    },
    "versionCityJSON": "2.0",
    "extraCityObjects": {
        "+SpaceSurfaceObject": {
            "allOf": [
                {
                    "$ref": "cityobjects.schema.json#/_AbstractCityObject"
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "+SpaceSurfaceObject"
                            ]
                        },
                        "attributes": {
                            "type": "object",
                            "properties": {
                                "objectName": {
                                    "type": "string"
                                },
                                "objectType": {
                                    "type": "string"
                                },
                                "registrationDate": {
                                    "type": "string"
                                },
                                "surfaceObjectID": {
                                    "type": "string"
                                }
                            }
                        },
                        "geometry": {
                            "type": "array",
                            "items": {
                                "oneOf": [
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiLineString"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiSurface"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/CompositeSurface"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/Solid"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/CompositeSolid"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiSolid"
                                    }
                                ]
                            }
                        }
                    },
                    "required": [
                        "type",
                        "geometry"
                    ]
                }
            ]
        },
        "+SpacePlanUnit": {
            "allOf": [
                {
                    "$ref": "cityobjects.schema.json#/_AbstractCityObject"
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "+SpacePlanUnit"
                            ]
                        },
                        "attributes": {
                            "type": "object",
                            "properties": {
                                "abovegroundDepth": {
                                    "type": "number"
                                },
                                "undergroundDepth": {
                                    "type": "number"
                                },
                                "planUseType": {
                                    "type": "string"
                                },
                                "planObjectID": {
                                    "type": "string"
                                }
                            }
                        },
                        "geometry": {
                            "type": "array",
                            "items": {
                                "oneOf": [
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiLineString"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiSurface"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/CompositeSurface"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/Solid"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/CompositeSolid"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiSolid"
                                    }
                                ]
                            }
                        }
                    },
                    "required": [
                        "type",
                        "geometry"
                    ]
                }
            ]
        },
        "+SpaceLegalParcel": {
            "allOf": [
                {
                    "$ref": "cityobjects.schema.json#/_AbstractCityObject"
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "+SpaceLegalParcel"
                            ]
                        },
                        "attributes": {
                            "type": "object",
                            "properties": {
                                "legalObjectID": {
                                    "type": "string"
                                },
                                "parcelUseType": {
                                    "type": "string"
                                }
                            }
                        },
                        "geometry": {
                            "type": "array",
                            "items": {
                                "oneOf": [
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiLineString"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiSurface"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/CompositeSurface"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/Solid"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/CompositeSolid"
                                    },
                                    {
                                        "$ref": "geomprimitives.schema.json#/MultiSolid"
                                    }
                                ]
                            }
                        }
                    },
                    "required": [
                        "type",
                        "geometry"
                    ]
                }
            ]
        },
        "+SpaceBuilding": {
            "allOf": [
                {
                    "$ref": "cityobjects.json#/_AbstractBuilding"
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "+SpaceBuilding"
                            ]
                        },
                        "attributes": {
                            "properties": {
                                "buildingID": {
                                    "type": "number"
                                },
                                "buildingName": {
                                    "type": "string"
                                },
                                "buildingObjectID": {
                                    "type": "string"
                                },
                                "buildingState": {
                                    "type": "string"
                                },
                                "buildingUseType": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "required": [
                        "type"
                    ]
                }
            ]
        },
        "+SpaceCommonArea": {
            "allOf": [
                {
                    "$ref": "#/+SpaceLegalParcel"
                },
                {
                    "properties": {
                        "type": {
                            "enum": [
                                "+SpaceCommonArea"
                            ]
                        },
                        "attributes": {
                            "properties": {
                                "areagUseType": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "required": [
                        "type", "geometry"
                    ]
                }
            ]
        }
    },
    "extraRootProperties": {
    },
    "extraSemanticSurfaces": {
    }
}